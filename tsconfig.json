{
    "compilerOptions": {
      "lib": [
        "dom",
        "es2021"
      ],
      "module": "ES6",
      "moduleResolution": "node",
      "target": "ES6",
      "strictNullChecks": true,
      "sourceMap": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
